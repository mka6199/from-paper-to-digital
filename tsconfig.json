{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-native",
    "moduleSuffixes": [".native", ".ios", ".android", ".web", ""]
  },
  "exclude": [
    "node_modules"
  ]
}
